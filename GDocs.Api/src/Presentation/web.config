<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<staticContent>
			<remove fileExtension=".json" />
			<mimeMap fileExtension=".json" mimeType="text/plain" />
		</staticContent>
		<security>
			<requestFiltering>
				<!-- This will handle requests up to 700MB (CD700) -->
				<requestLimits maxAllowedContentLength="737280000" />
			</requestFiltering>
		</security>
	</system.webServer>
</configuration>